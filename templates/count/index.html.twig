{% extends 'base.html.twig' %}

{% block title %}Hello CountController!
{% endblock %}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
		#count {


			background-color: rgb(238, 144, 152);
			padding: 40px;
			box-shadow: 10px 5px 5px black;
			font-size: 35px;
			width: 742px;
			border-radius: 12px;
			position: absolute;
			left: 200px;
			top: 200px;

		}


		#lastid {


			background-color: rgb(20, 155, 165);
			padding: 60px;
			box-shadow: 10px 5px 5px black;
			font-size: 35px;
			width: 742px;
			border-radius: 12px;
			position: absolute;
			left: 200px;
			top: 600px;


		}

		#pied {

			height: 800px;
			width: 600px;
			position: absolute;
			left: 1150px;
			border-radius: 20px;
			top: 100px;


		}


		#pied:hover {


			border-radius: 25px;
			box-shadow: 7px 4px 4px pink;

		}
	</style>

	<div class="example-wrapper">

		<img id="" src="../img/pied.jpg" alt="photo pied et talon rouge">
	</div>
{% endblock %}
